# Streamlit Secrets Template
# Copy this file to .streamlit/secrets.toml and fill in your actual values
# DO NOT commit .streamlit/secrets.toml to git

[groq]
api_key = "your_groq_api_key_here"

[api]
base_url = "http://localhost:8000"

[app]
debug = false
environment = "production"

# For Streamlit Cloud deployment, add these secrets via the web interface
# instead of creating the secrets.toml file 